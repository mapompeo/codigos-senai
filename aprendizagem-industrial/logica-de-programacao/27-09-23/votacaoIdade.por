programa
{
	funcao inicio()
	{
		inteiro idade = 0

		escreva("Digite sua idade ")
		leia(idade)

		se (idade >= 16) {
			escreva("Está apto para voltar!")
		}
		senao {
			escreva("Não pode votar!")
		}
	}
}
