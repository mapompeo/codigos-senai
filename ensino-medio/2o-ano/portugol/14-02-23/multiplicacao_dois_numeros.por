programa
{
	funcao inicio()
	{
		inteiro num1 = 0, num2 = 0, res = 0

		escreva("---------------------------------------\n")
		escreva("  Multiplicação de 2 números inteiros  \n")
		escreva("---------------------------------------\n")
		escreva("Digite um número inteiro               \n")
		leia(num1)
		escreva("Digite o segundo número inteiro        \n")
		leia(num2)
		res = num1 * num2
		escreva("O resultado da multiplicação é = ",res)
		escreva("\n")
	}
}
